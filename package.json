{
  "name": "la-hache-gourmande",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node server.js",
    "start:local": "ng serve -o",
    "start:local:en": "ng serve --configuration=en -o",
    "css-build": "node-sass --omit-source-map-url src/styles.scss src/styles.css",
    "build": "ng build",
    "test": "ng test",
    "lint": "eslint . --ext js,ts,json",
    "lint:fix": "eslint . --ext js,ts,json --quiet --fix",
    "extract:i18n": "ng xi18n --output-path src/app/locale",
    "e2e": "ng e2e",
    "heroku-postbuild": "for lang in en; do ng build --output-path=dist/$lang --prod --base-href /$lang/ --locales=src/app/locale/messages.$lang.xlf --localize=$lang; done"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.3",
    "@angular/cli": "~9.0.4",
    "@angular/common": "~9.0.3",
    "@angular/compiler": "~9.0.3",
    "@angular/compiler-cli": "~9.0.3",
    "@angular/core": "~9.0.3",
    "@angular/forms": "~9.0.3",
    "@angular/localize": "^9.1.3",
    "@angular/platform-browser": "~9.0.3",
    "@angular/platform-browser-dynamic": "~9.0.3",
    "@angular/router": "~9.0.3",
    "@fortawesome/angular-fontawesome": "^0.7.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "animate.css": "^4.1.0",
    "bulma": "^0.8.0",
    "bulma-carousel": "^4.0.4",
    "bulma-o-steps": "^1.0.9",
    "core-js": "^3.6.4",
    "express": "^4.17.1",
    "express-sslify": "^1.2.0",
    "node-sass": "^4.14.1",
    "path": "^0.12.7",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "typescript": "~3.7.5",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.901.9",
    "@angular/language-service": "~9.0.3",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "codelyzer": "^5.1.2",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "prettier": "^2.1.0",
    "prettier-eslint": "^11.0.0",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "webpack": "^4.42.0",
    "webpack-dev-server": "^3.11.0"
  },
  "engines": {
    "node": "12.16.1",
    "npm": "6.13.4"
  }
}
